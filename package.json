    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.43.5",
    "typescript": "^4.5.2",
    "vite": "^2.6.4",
    "vite-plugin-components": "^0.13.3",
    "vite-plugin-pages": "^0.18.2",
    "vite-plugin-vuedoc": "^3.1.3",
    "vue": "^3.2.16",
