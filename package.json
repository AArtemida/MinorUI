{
  "name": "v-minor-ui",
  "version": "0.1.4",
  "main": "lib/minorUi.js",
  "scripts": {
    "dev": "vite",
    "build": "yarn clean && yarn build:theme && yarn build:package",
    "serve": "vite preview",
    "test": "jest --runInBand",
    "lib": "vite build --target lib --name v-minor-ui --dest lib packages/index.ts",
    "build:theme": "gulp build --gulpfile packages/themes/gulpfile.js",
    "build:package": "rollup -c rollup.config.js",
    "clean": "rimraf ./lib ./dist"
  },
  "keywords": [
    "vue3",
    "vite",
    "component",
    "ui"
  ],
  "files": [
    "lib",
    "package.json",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./lib/minorUi.js"
    },
    "./lib/*.js": "./lib/*.js",
    "./*": "./*"
  },
  "resolutions": {
    "node-sass": "npm:sass@1.43.5"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@babel/plugin-proposal-optional-chaining": "^7.16.0",
    "@babel/preset-env": "^7.19.0",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-image": "^2.1.1",
    "@types/markdown-it-container": "^2.0.5",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/compiler-sfc": "^3.2.23",
    "@vue/test-utils": "^2.0.0-rc.17",
    "babel": "^6.23.0",
    "babel-jest": "^27.3.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-preset-env": "^1.7.0",
    "del": "^6.0.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.1.1",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-cssmin": "^0.2.0",
    "gulp-sass": "^5.0.0",
    "jest": "^27.3.1",
    "markdown-it-container": "^3.0.0",
